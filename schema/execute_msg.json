{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "deposit_msg"
      ],
      "properties": {
        "deposit_msg": {
          "type": "object",
          "required": [
            "commitment"
          ],
          "properties": {
            "commitment": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "with_draw_msg"
      ],
      "properties": {
        "with_draw_msg": {
          "type": "object",
          "required": [
            "nullifier_hash",
            "proof",
            "recipient",
            "root"
          ],
          "properties": {
            "nullifier_hash": {
              "type": "string"
            },
            "proof": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "maxItems": 8,
              "minItems": 8
            },
            "recipient": {
              "type": "string"
            },
            "root": {
              "type": "string"
            }
          }
        }
      }
    }
  ]
}
